/*
 * DAOFactory template metadata.
 */
// The template name (the filename).
name : DAOFactory.stg;

// The type of template: per-table, per-repository, per-foreign-key, per-custom-result, per-custom-sql.
type : per-table;

// The output flavor.
output: Java;

// The filename builder.
filename  builder : <engine.name.voName><tableName.voName>DAOFactory.java;

// The package.
package: <packageName>.dao.<tableName.normalized.lowercased>.<engine.name.normalized.lowercased>;