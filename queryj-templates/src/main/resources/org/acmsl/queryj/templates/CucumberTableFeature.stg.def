/*
 * CucumberTableFeature template metadata.
 */
// The template name (the filename).
name : CucumberTableFeature.stg;

// The type of template: per-table, per-repository, per-foreign-key, per-custom-result, per-custom-sql.
type : per-table;

// The output flavor.
output: Cucumber;

// The filename builder.
filename  builder : <tableName.singular.camelCase>.feature;

// The package.
package: <packageName>;