/*
 * DBSD template metadata.
 */
// The template name (the filename).
name : DBSD.stg;

// The type of template: per-table, per-repository, per-foreign-key, per-custom-result, per-custom-sql.
type : per-repository;

// The output flavor.
output: DBSD;

// The filename builder.
filename  builder : <repository.capitalized>.dbsd;

// The package.
package: <nothing>;