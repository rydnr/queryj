/*
 * Template metadata.
 */
// The template name (the filename).
name : MyTemplate.stg;

// The type of template: per-table, per-repository, per-foreign-key, per-custom-result, per-custom-sql.
type : per-table;

// The output flavor.
output: Java;

// The filename builder.
filename builder : <engine.name><tableName.singular.camelCase>Template.java;

// The package.
package: <packageName>.dao.rdb.<engine.name.lowercased>;